{
    "name": "@iobuhovxyz/tinyflow",
    "version": "0.2.2",
    "author": "Illia Obukhau <pluto.kuiper.zofaa@slmail.me>",
    "description": "Zero dependency library for writing type safe, organized workflows for node. Inspired by GitHub actions.",
    "keywords": [
        "typescript",
        "flow",
        "tinyflow",
        "workflow",
        "job",
        "task",
        "runner",
        "node"
    ],
    "repository": {
        "type": "git",
        "url": "git+https://github.com/iobuhov/tinyflow.git"
    },
    "license": "Apache-2.0",
    "type": "module",
    "exports": {
        "./*.mjs": {
            "types": "./dist/*.d.mts",
            "default": "./dist/*.mjs"
        }
    },
    "scripts": {
        "build": "tsc --build --verbose",
        "dev": "tsc --build --verbose --watch",
        "clean": "rimraf dist",
        "test": "vitest"
    },
    "packageManager": "pnpm@9.12.3+sha512.cce0f9de9c5a7c95bef944169cc5dfe8741abfb145078c0d508b868056848a87c81e626246cb60967cbd7fd29a6c062ef73ff840d96b3c86c40ac92cf4a813ee",
    "devDependencies": {
        "@eslint/js": "^9.14.0",
        "@types/eslint__js": "^8.42.3",
        "@types/node": "^22.9.0",
        "eslint": "^9.14.0",
        "globals": "^15.12.0",
        "prettier": "^3.3.3",
        "rimraf": "^6.0.1",
        "typescript": "^5.6.3",
        "typescript-eslint": "^8.14.0",
        "vitest": "^2.1.5"
    }
}
